  "scripts": {
    "start": "cross-env PORT=6003 REACT_APP_GIT_HASH=$(git rev-parse --short HEAD) craco start",
    "start:prod": "pnpm build:prod && cross-env PORT=6003 serve -s build",
    "build": "pnpm run clean && cross-env CLIENT_ENV=development GENERATE_SOURCEMAP=true craco build",
    "build:prod": "pnpm run clean && npm run compile",
    "compile": "cross-env CLIENT_ENV=production GENERATE_SOURCEMAP=false REACT_APP_GIT_HASH=$(git rev-parse --short HEAD) craco build",
    "compile:prod": "cross-env CLIENT_ENV=production GENERATE_SOURCEMAP=false craco build",
    "clean": "rm -rf build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dev": "yarn && yarn build && yarn start",
    "watched": "nodemon -w src --ext js --exec npm run build",
    "watch": "npm-watch",
    "g": "npx generate-react-cli component --",
    "p": "npx generate-react-cli component -- --type=page",
    "upload": " aws s3 sync build s3://zwf-web/",
    "analyze": "cross-env CLIENT_ENV=production GENERATE_SOURCEMAP=true craco build && source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^4.2.4",
    "babel-plugin-import": "^1.13.0",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "prop-types": "^15.7.2",
    "react-scripts": "5.0.1",
    "serve": "^12.0.0",
    "source-map-explorer": "^2.5.2"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  }
}
